{
  "name": "de.htw-berlin.centis.vr-sketching-geometry",
  "version": "0.2.4",
  "displayName": "VR Sketching Geometry",
  "description": "Lightweight VR Sketching Tool",
  "unity": "2022.3",
  "documentationUrl": "hunsri.github.io/VRSketchingGeometryUP/Assets/VRSketchingGeometryPackage/README.html",
